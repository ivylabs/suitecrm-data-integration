//var is_leap_year_for_week_iso8601 = (days_in_year_for_week_iso8601==366)?1:0;

var QUARTER_SHORT_DESC = "Q"+QUARTER2;
var QUARTER_LONG_DESC = "QTR "+QUARTER2;

var YYYYMM = str2num(YYYYMMSTR);

var YYYYQQ = YEAR4 + "-0" + QUARTER2;
var YYYYQQQ = YEAR4 + " Q" + QUARTER2;
var YYYYQQQQ = YEAR4 + " QTR " + QUARTER2;

var YYYYQQQMM;

if (MONTH2 &lt;10) { 
	YYYYQQQMM = YEAR4 + " Q" + QUARTER2 + " 0" + MONTH2;
} else { 
	YYYYQQQMM = YEAR4 + " Q" + QUARTER2 + " " + MONTH2;
}

var YYYYQQQMMDD;

if (DAY_OF_MONTH2 &lt;10) {
