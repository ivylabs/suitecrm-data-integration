        <jsScript_script>//Script here

var DAYS = 0;
var STATE = "CLOSED";
var EVENT_COUNT = -1;

if(CLONE_SEQUENCE % 2 ==0){
	STATE = "OPEN";
	EVENT_COUNT = 1
}

if(NEW_STATUS == "Prospecting" &amp;&amp; STATE == "CLOSED"){
	DAYS = QUALIFICATION;
} else if(NEW_STATUS == "Qualification" &amp;&amp; STATE == "OPEN"){
	DAYS = QUALIFICATION;
} else if(NEW_STATUS == "Qualification" &amp;&amp; STATE == "CLOSED"){
	DAYS = NEEDS_ANALYSIS;
} else if(NEW_STATUS == "Needs Analysis" &amp;&amp; STATE == "OPEN"){
	DAYS = NEEDS_ANALYSIS;
} else if(NEW_STATUS == "Needs Analysis" &amp;&amp; STATE == "CLOSED"){
	DAYS = VALUE_PROPOSITION;
} else if(NEW_STATUS == "Value Proposition" &amp;&amp; STATE == "OPEN"){
	DAYS = VALUE_PROPOSITION;
} else if(NEW_STATUS == "Value Proposition" &amp;&amp; STATE == "CLOSED"){
	DAYS = IDENTIFY_DECISION_MAKERS;
} else if(NEW_STATUS == "Identifying Decision Makers" &amp;&amp; STATE == "OPEN"){
	DAYS = IDENTIFY_DECISION_MAKERS;
} else if(NEW_STATUS == "Identifying Decision Makers" &amp;&amp; STATE == "CLOSED"){
	DAYS = PERCEPTION_ANALYSIS;
} else if(NEW_STATUS == "Perception Analysis" &amp;&amp; STATE == "OPEN"){
	DAYS = PERCEPTION_ANALYSIS;
} else if(NEW_STATUS == "Perception Analysis" &amp;&amp; STATE == "CLOSED"){
	DAYS = PROPOSAL_PRICE_QUOTE;
} else if(NEW_STATUS == "Proposal/Price Quote" &amp;&amp; STATE == "OPEN"){
	DAYS = PROPOSAL_PRICE_QUOTE;
} else if(NEW_STATUS == "Proposal/Price Quotes" &amp;&amp; STATE == "CLOSED"){
	DAYS = NEGOTIATION_REVIEW;
} else if(NEW_STATUS == "Negotiation/Review" &amp;&amp; STATE == "OPEN"){
	DAYS = NEGOTIATION_REVIEW;
